        <script type="text/javascript">
            var __clientSettings = {{{ clientSettings }}};    
            
            var authIdCallback;
            function __addAuthIdCallback(cb){
                authIdCallback = cb;
            }
            function __setAuthId(idToken, href){
                authIdCallback && authIdCallback(idToken, href);
            }

            var authCodeCallback;
            function __addAuthCodeCallback(cb){
                authCodeCallback = cb;
            }
            function __setAuthCode(code, href){
                authCodeCallback && authCodeCallback(code, href);
            }
        </script>
            
        <script type="text/javascript" src="{{ cdn_path }}/hdx/components/respond/respond.min.js"></script>
        <script type="text/javascript" src="{{ cdn_path }}/hdx/components/modernizr/modernizr.js"></script>
