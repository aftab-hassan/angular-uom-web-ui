        <script type="text/javascript" src="{{ cdn_path }}/hdx/js/require.config.js"></script>
        <script type="text/javascript">
            require.config({ baseUrl: '{{ cdn_path }}/hdx' });
        </script>
        <script type="text/javascript">
            // Preload JS libraries and exec page scripts
            require([
                'jquery',
                'ge-bootstrap',
                'navbar-offcanvas'
            ], function($) {});
        </script>
        <style> iframe.auth{ visibility:hidden; position:absolute; height:0px ;width:0px; } </style>
